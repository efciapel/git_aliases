# Git aliases
alias goto='git checkout'
alias ciagnij='git pull'
alias pchaj='git push'
alias coje='git status'
alias czekej='git commit --amend'
alias pierwszy="git push --set-upstream origin $(git branch | grep '*' | sed s/'* '//)"
alias kajjestem="git branch | grep '*'"
alias komit='git commit -m'
alias stos='git stash'
alias odswiez='git checkout master; git pull; git checkout -; git rebase master'
